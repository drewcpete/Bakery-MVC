@using Bakery.Models;

<h1>All Vendors:</h1>
@if(Model.Count == 0 )
{
    <p>You currently have no vendors!</p>
}
@foreach(Vendor vendor in Vendors)
{
    <p><a href="/vendors/@vendors.Id">@vendor.VendorName</a></p>
}

<a href="/home">Go Home!</a>
<a href="/orders">Check out all orders!</a>
<a href="/vendors">Check out all vendors!</a>